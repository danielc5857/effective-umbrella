function setup() {
  createCanvas(700, 700);
}

function draw() {
  background(220);
	//body
  strokeWeight(2)
  fill(80)
  rect(245,175,255,350)
  //head
  fill(80)
	rect(275,75,200,100)
	//mouth
  strokeWeight(5)
  fill("yellow")
  rect(325,125,100,50)
	line(325,150,425,150)
  line(350,175,350,125)
  line(375,175,375,125)
  line(400,175,400,125)
  //eyes
  fill(225,0,0)
  strokeWeight(2)
  ellipse(325,100,40,40)
  fill(0,0,0)
  ellipse(427,100,40,40)
  line(400,75,475,150)
  //arms
  fill(80)
	strokeWeight(2)
  rect(190,210,50,150)
  rect(505,210,50,150)
  strokeWeight(0)
  //left arm
  rect(225,210,50,65)
  //right arm
  rect(465,210,50,65)
  //antennas
  strokeWeight(7)
  line(325,72,325,50)
	line(420,72,420,50)
  ellipse(420,49,30,15)
  ellipse(325,49,30,15)
  //leg
  fill(80)
  strokeWeight(2)
  rect(250,525,50,125)
  strokeWeight(0)
  rect(250,500,50,150)
  strokeWeight(2)
  rect(445,525,50,125)
  strokeWeight(0)
  rect(445,500,50,150)
	//text on chest
	fill(0,0,0)
  textFont("Bold")
  textSize(25)
  text("what is my purpose?",50,150)
  //torso
  strokeWeight(8)
  line(250,300,495,200)
  line(250,350,495,250)
  fill(80)
  strokeWeight(2)
	ellipse(425,253,30,30)
  ellipse(350,283,30,30)
}
    </script>
